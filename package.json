  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "component",
    "vue.js"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-vue": "^5.2.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.3",
